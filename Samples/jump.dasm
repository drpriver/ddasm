dlimport C
  "libc.so.6"
  puts 1 1
end
function start
  scmp rarg1 4
  jump ge label bad
  scmp rarg1 0
  jump lt label bad
  move r2 [label a label b label c label d]
  mul rarg1 rarg1 0p1
  add r2 r2 rarg1
  read rip r2
  label a
    move rarg1 "a"
    call function C.puts
    halt
  label b
    move rarg1 "b"
    call function C.puts
    halt
  label c
    move rarg1 "c"
    call function C.puts
    halt
  label d
    move rarg1 "d"
    call function C.puts
    halt
  label bad
    move rarg1 "bad input"
    call function C.puts
    halt
end
