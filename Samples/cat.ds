function start(in, out){
    if(!in) in = GetStdIn();
    if(!out) out = GetStdOut();
    fcopy(in, out);
}
function fcopy(in, out){
    let ptr = Malloc(32768);
    let nread = 1;
    for(;nread;){
        nread = Fread(ptr, 1, 32768, in);
        Fwrite(ptr, 1, nread, out);
    }
    Fflush(out);
    Free(ptr);
}
